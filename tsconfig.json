{
  "compilerOptions": {
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "dom"
    ],
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "module": "es6"
  }
}